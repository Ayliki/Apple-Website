{
    "extends": [
      "standart",
      "plugin:react/recommended",
      "plugin:tailwindcss/recommended",
      "prettier"
    ],
    "rules": {
      "no-multiple-empty-lines": [
        "error",
        {
            "max": 1,
            "maxEOF": 1
        }
      ],
      "object-curly-newline": 0
    }
  }